{
  "name": "movie-collection",
  "version": "0.0.1",
  "description": "Movie Collection",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline --content-base build/ -d",
    "test": "./node_modules/.bin/mocha app/**/**/**/*.spec.js --require tests/setup.js --require babel-register --require ignore-styles",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm test",
    "build": "./node_modules/.bin/webpack -p",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "app/**/*.spec.js"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasBassetti/movie-collection"
  },
  "keywords": [
    "movie",
    "collection"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasBassetti/movie-collection/issues"
  },
  "homepage": "https://github.com/LucasBassetti/movie-collection#readme",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "nyc": "^10.2.0",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.5.0",
    "redux-mock-store": "^1.2.3",
    "sinon": "^2.3.4",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "random-id": "0.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "styled-components": "^2.0.1"
  }
}
